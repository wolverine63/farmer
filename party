#include<bits/stdc++.h>
using namespace std;
#define MAX 501

int getId(int M[MAX][MAX],int n);

int main()
{
    int M[MAX][MAX];
    int N;
    cin>>N;
    memset(M,0,sizeof M);
    for(int i=0;i<N;i++)
    {
        for(int j=0;j<N;j++)
          {
             cin>>M[i][j];
          }
    }
        cout<<getId(M,N)<<endl;

    }

    int getId(int M[MAX][MAX], int n)
{
   int i,j,q;
   int c=0;
   for(i=0;i<n;i++)
   {
    c=0;
       for(j=0;j<n;j++)
       {
           if(M[i][j]==0)
           c++;
           else break;

       }

      // cout<<c<<" ";
       if(c==n){
       q=i;break;}
       else {continue;}
   }
   c=0;
   for(i=0;i<n;i++)
   {
       if(i==q)
       continue;
       else
       {
        if(M[i][q]==1)
        c++;
       }

   }
    if(c==n-1)
    return q;
    else
    return -1;


}
